    "pluralName": "events",
    "displayName": "event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "tittle": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": false
    },
    "eventDate": {
      "type": "date",
      "required": true
    },
    "eventPlace": {
      "type": "string",
      "required": true
    },
    "eventPhotos": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "events"
    },
    "featureImage": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true
    }
  }
